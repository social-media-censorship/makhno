{
  "name": "makhno-server",
  "version": "0.1.1",
  "description": "Makhno is a server side package to check social media content availability",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "detour": "DEBUG=* bin/detour-everything.mjs",
    "submission": "DEBUG=*,-express:*,-body-parser:* bin/submission.mjs",
    "scheduled": "DEBUG=*,-express:*,-body-parser:* bin/scheduled.mjs",
    "gafam": "DEBUG=*,-express:*,-body-parser:*,-utils:parsinghq* bin/gafam.mjs",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/makhno.git"
  },
  "keywords": [
    "social-media",
    "scraping",
    "censorship",
    "observatory",
    "content-takedown"
  ],
  "author": "Claudio Agosti (github.com/vecna)",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/tracking-exposed/makhno/issues"
  },
  "homepage": "https://github.com/tracking-exposed/makhno#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "jsdom": "^20.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^4.10.0",
    "zx": "^7.1.0"
  },
  "devDependencies": {
    "jest": "^29.1.2"
  }
}
